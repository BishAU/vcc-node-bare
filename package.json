{
  "name": "vcc-abs-platform",
  "version": "1.0.0",
  "description": "Volunteer Community Collaboration Australian Bureau of Statistics Platform",
  "type": "module",
  "scripts": {
    "start": "node dist/server/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "npm run build:frontend && npm run build:server",
    "build:frontend": "vite build",
    "build:server": "tsc src/server.ts --outDir dist/server",
    "migrate:staging": "./scripts/db-migrate.sh staging up",
    "migrate:production": "./scripts/db-migrate.sh production up",
    "backup:staging": "./scripts/db-backup.sh staging",
    "backup:production": "./scripts/db-backup.sh production",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@logtail/node": "^0.4.0",
    "@logtail/winston": "^0.4.0",
    "@prisma/client": "^5.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "next-auth": "^4.22.3",
    "prom-client": "^14.2.0",
    "ts-node": "^10.9.2",
    "winston": "^3.10.0",
    "cross-spawn": "^7.0.3",
    "tough-cookie": "^4.1.3",
    "xero-node": "^9.3.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.45.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.31",
    "prisma": "^5.0.0",
    "tailwindcss": "^3.3.5",
    "@tailwindcss/forms": "^0.5.7",
    "typescript": "^5.1.6",
    "vite": "^4.4.7"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
