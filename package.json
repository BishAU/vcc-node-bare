{
  "name": "vcc-release",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "prisma": {
    "seed": "ts-node --esm prisma/seed.ts"
  },
  "scripts": {
    "dev": "vite",
    "server": "nodemon server/src/app.js",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest --config=jest.config.cjs",
    "test:coverage": "vitest run --coverage",
    "test:stripe": "jest tests/stripe.test.js --config=jest.config.cjs --testTimeout=30000",
    "test:watch": "jest --config=jest.config.cjs --watch",
    "test:xero": "jest tests/xero.test.js --config=jest.config.cjs --testTimeout=30000",
    "db:seed": "prisma db seed",
    "start": "node server/src/app.js",
    "test:create-invoices": "node tests/create-test-invoices.js"
  },
  "dependencies": {
