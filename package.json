{
  "name": "vcc-abs-platform",
  "version": "1.0.0",
  "description": "Volunteer Community Collaboration Australian Bureau of Statistics Platform",
  "type": "module",
  "scripts": {
    "start": "node dist/server/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "npm run build:frontend && npm run build:server && npm run postbuild",
    "build:frontend": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "postbuild": "node scripts/postbuild.js",
    "generate:prisma": "prisma generate",
    "migrate:staging": "./scripts/db-migrate.sh staging up",
    "migrate:production": "./scripts/db-migrate.sh production up",
    "backup:staging": "./scripts/db-backup.sh staging",
    "backup:production": "./scripts/db-backup.sh production",
    "test": "jest",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@logtail/node": "^0.4.0",
    "@logtail/winston": "^0.4.0",
    "@prisma/client": "^5.0.0",
    "@tanstack/react-query": "^4.29.19",
    "cors": "^2.8.5",
    "cross-spawn": "^7.0.3",
    "date-fns": "^4.1.0",
    "express": "^4.18.2",
    "framer-motion": "^11.11.17",
    "helmet": "^7.1.0",
    "next-auth": "^4.22.3",
    "prom-client": "^14.2.0",
    "react": "^18.2.0",
    "react-countup": "^6.5.3",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.53.2",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.3.0",
    "react-intersection-observer": "^9.13.1",
    "react-paginate": "^8.2.0",
    "react-router-dom": "^6.20.0",
    "recharts": "^2.13.3",
    "tough-cookie": "^4.1.3",
    "winston": "^3.10.0",
    "xero-node": "^9.3.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.31",
    "prisma": "^5.0.0",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
