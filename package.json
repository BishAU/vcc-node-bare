    "@types/react-paginate": "^7.1.4",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "chart.js": "^4.4.1",
    "cors": "^2.8.5",
    "cross-spawn": "^7.0.3",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.21.1",
    "framer-motion": "^10.18.0",
    "helmet": "^8.0.0",
